{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/forms": "off",
    "axe/name-role-value": "off",
    "no-inline-styles": "off",
    "apple-touch-icons": "off",
    "axe/parsing": [
      "default",
      {
        "duplicate-id-active": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-webkit-text-size-adjust"
        ]
      }
    ]
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_ff <= 126",
    "not firefox <= 126",
    "not safari <= 17.5"
  ]
}